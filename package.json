{
  "name": "ethpandaops-notebooks",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "node -r dotenv/config node_modules/.bin/evidence build",
    "build:strict": "node -r dotenv/config node_modules/.bin/evidence build:strict",
    "dev": "node -r dotenv/config node_modules/.bin/evidence dev --port 9847",
    "test": "node -r dotenv/config node_modules/.bin/evidence build",
    "sources": "node -r dotenv/config node_modules/.bin/evidence sources",
    "sources:strict": "node -r dotenv/config node_modules/.bin/evidence sources --strict",
    "preview": "node -r dotenv/config node_modules/.bin/evidence preview"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=18.0.0"
  },
  "type": "module",
  "dependencies": {
    "@evidence-dev/core-components": "^5.4.0",
    "@evidence-dev/duckdb": "^2.0.0",
    "@evidence-dev/evidence": "^40.1.6",
    "dotenv": "^17.2.3",
    "evidence-connector-clickhouse": "^0.0.2",
    "cached-clickhouse-connector": "*"
  },
  "overrides": {
    "axios": "^1.7.4"
  }
}
